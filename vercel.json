{
  "framework": "vite",
  "buildCommand": "rm -rf node_modules package-lock.json && npm install && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --force",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
